<!-- FLAPS LEVER -->


<animation>
        <type>pick</type>
        <object-name>FlapLever</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>nasal</command>
                <script>controls.flapsDown(1);</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>FlapLever</object-name>
