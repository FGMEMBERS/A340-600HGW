<PropertyList>

<path>ai.ac</path>


<animation>
  <type>scale</type>
  <x-offset>0.7</x-offset>
  <y-offset>0.7</y-offset>
  <z-offset>0.7</z-offset>
 </animation>




    <animation>
        <type>select</type>
        <object-name>gs.flag</object-name>
        <condition>
			<or>
				<not>
					<property>instrumentation/nav/has-gs</property>
				</not>
				<not>
					<property>instrumentation/nav/in-range</property>
				</not>
			</or>
        </condition>
        <property>orientation/roll-deg</property>
    </animation>

    <animation>
        <type>select</type>
        <object-name>loc.flag</object-name>
        <condition>
			<not>
				<property>instrumentation/nav/in-range</property>
			</not>
		</condition>
        <property>orientation/roll-deg</property>
    </animation>

<animation>
        <type>select</type>
        <object-name>Plane.111</object-name>
        <condition>
			<property>instrumentation/afds/inputs/FD</property>
		
		</condition>
    </animation>



    <animation>
        <type>rotate</type>
        <object-name>AIball</object-name>
        <object-name>AI.ring</object-name>
		  <property>orientation/roll-deg</property>
        <center>
            <x-m>-0.041</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AIball</object-name>
        <property>orientation/pitch-deg</property>
        <center>
            <x-m>-0.041</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>AI.slipball</object-name>
        <property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
			<interpolation>
         	   <entry><ind>-1</ind><dep>-8</dep></entry>
       	     <entry><ind>0</ind><dep>0</dep></entry>
          	  <entry><ind>1</ind><dep>8</dep></entry>
     	   </interpolation>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0.02</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>LOC.needle</object-name>
        <property>instrumentation/nav/heading-needle-deflection</property>
			<interpolation>
            <entry><ind>-5</ind><dep>-0.0124</dep></entry>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>5</ind><dep>0.0124</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>AI.gsneedle</object-name>
        <property>instrumentation/nav/gs-needle-deflection</property>
			<interpolation>
            <entry><ind>-2.0</ind><dep>-0.0155</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2.0</ind><dep>0.0155</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>turn.needle</object-name>
        <property>instrumentation/turn-indicator/indicated-turn-rate</property>
			<interpolation>
            <entry><ind>-2</ind><dep>-0.007</dep></entry>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>2</ind><dep>0.007</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>aoa.needle</object-name>
        <property>orientation/alpha-deg</property>
			<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>5.60</ind><dep>0.004</dep></entry><!-- 0.4-->
			<entry><ind>8.4</ind><dep>0.0166</dep></entry><!-- 0.6 - optimal appr-->
			<entry><ind>11.2</ind><dep>0.0292</dep></entry><!-- 0.8-->
            <entry><ind>14</ind><dep>0.0334</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>

  <animation>
    <type>material</type>
    <object-name>AI</object-name>
    <object-name>AI.002</object-name>
    <!--object-name>AI.face</object-name-->
    <object-name>AI.gsneedle</object-name>
    <!--object-name>AI.ring</object-name-->
    <!--object-name>AI.slipball</object-name-->
    <object-name>AIball</object-name>
    <!--object-name>Circle.016</object-name-->
    <!--object-name>Cylinder.001</object-name-->
    <object-name>LOC.needle</object-name>
	<!--object-name>Plane.109</object-name-->
	<!--object-name>Plane.110</object-name-->
	<!--object-name>Plane.111</object-name-->
	<object-name>aoa.needle</object-name>
	<object-name>gs.flag</object-name>
	<object-name>loc.flag</object-name>
	<object-name>turn.needle</object-name>
    <emission>
      <red>   0.3 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
	<factor-prop>controls/lighting/instruments-norm</factor-prop>
    </emission>
  </animation>

<animation>
        <type>select</type>
       <!--object-name>AI</object-name-->
    <object-name>AI.002</object-name>
    <!--object-name>AI.face</object-name-->
    <object-name>AI.gsneedle</object-name>
    <!--object-name>AI.ring</object-name-->
    <!--object-name>AI.slipball</object-name-->
    <!--object-name>AIball</object-name-->
    <!--object-name>Circle.016</object-name-->
    <!--object-name>Cylinder.001</object-name-->
    <object-name>LOC.needle</object-name>
	<object-name>Plane.109</object-name>
	<!--object-name>Plane.110</object-name-->
	<!--object-name>Plane.111</object-name-->
	<object-name>aoa.needle</object-name>
	<object-name>gs.flag</object-name>
	<object-name>loc.flag</object-name>
	<object-name>turn.needle</object-name>
        <condition>
				<and>
					<property>/systems/electrical/outputs/avionics</property>
				</and>
			</condition>
		</animation>

<animation>
        <type>rotate</type>
        <object-name>Cylinder.001</object-name>
        <property>instrumentation/nav/radials/selected-deg</property>
        <factor>2</factor>
        <center>
            <x-m>0.02</x-m>
            <y-m>-0.05</y-m>
            <z-m>-0.05</z-m>
        </center>
		<axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<animation>
		<type>pick</type>
		<object-name>Cylinder.001</object-name>
		<action>
			<button>4</button> <!--  scroll down -->
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav/radials/selected-deg</property>
				<step type="int">-1</step>
				<min type="int">1</min>
				<max type="int">361</max>
				<wrap type="bool">true</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.click(5);</script>
			</binding>
		</action>
	</animation>

   <animation>
		<type>pick</type>
		<object-name>Cylinder.001</object-name>
		<action>
			<button>3</button> <!--  scroll up -->
			<repeatable>true</repeatable>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav/radials/selected-deg</property>
				<step type="int">1</step>
				<min type="int">1</min>
				<max type="int">361</max>
				<wrap type="bool">true</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.click(5);</script>
			</binding>
		</action>
	</animation>


<!--nav 2-->

<animation>
        <type>rotate</type>
        <object-name>Cylinder</object-name>
        <property>instrumentation/nav[1]/radials/selected-deg</property>
        <factor>2</factor>
        <center>
            <x-m>0.01</x-m>
