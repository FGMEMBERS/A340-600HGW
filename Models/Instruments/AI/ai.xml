		<object-name>Cylinder</object-name>
		<action>
			<button>4</button> 
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav[1]/radials/selected-deg</property>
				<step type="int">-1</step>
				<min type="int">1</min>
				<max type="int">361</max>
				<wrap type="bool">true</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.click(5);</script>
			</binding>
		</action>
	</animation>

   <animation>
		<type>pick</type>
		<object-name>Cylinder</object-name>
		<action>
			<button>3</button> 
			<repeatable>true</repeatable>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav[1]/radials/selected-deg</property>
				<step type="int">1</step>
				<min type="int">1</min>
				<max type="int">361</max>
				<wrap type="bool">true</wrap>
			</binding>
			<binding>
				<command>nasal</command>
				<script>controls.click(5);</script>
			</binding>
		</action>
	</animation>










<!-- LoD -->
 <animation>
   <type>range</type>
   <min-m>0</min-m>
   <max-m>20</max-m>
 </animation>


</PropertyList>
