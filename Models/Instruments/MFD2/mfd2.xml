<!--

A340-600HGW Navigation display
-->

<PropertyList>

 <path>EFIS.ac</path>

 <animation>
  <type>scale</type>
  <x-offset>0.75</x-offset>
  <y-offset>0.75</y-offset>
  <z-offset>0.75</z-offset>
 </animation>

<animation>
        <type>pick</type>
        <object-name>EFIS.bezel</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
		<script>dialogs.yokechart.open()</script>
            </binding>
        </action>
    </animation>
 


    <panel>
        <path>Aircraft/A340-600HGW/Models/Instruments/MFD2/mfd-panel.xml</path>
        <bottom-left>
            <x-m>0.003</x-m>
            <y-m>-0.090</y-m>
            <z-m>-0.087</z-m>
        </bottom-left>
        <bottom-right>
            <x-m>0.003</x-m>
            <y-m>0.090</y-m>
            <z-m>-0.087</z-m>
        </bottom-right>
        <top-left>
            <x-m>0.003</x-m>
            <y-m>-0.090</y-m>
            <z-m>0.094</z-m>
        </top-left>
    </panel>

    <panel>
        <path>Aircraft/A340-600HGW/Models/Instruments/MFD2/mfdtext-panel.xml</path>
        <bottom-left>
            <x-m>0.005</x-m>
            <y-m>-0.090</y-m>
            <z-m>-0.087</z-m>
        </bottom-left>
        <bottom-right>
            <x-m>0.005</x-m>
            <y-m>0.090</y-m>
            <z-m>-0.087</z-m>
        </bottom-right>
        <top-left>
            <x-m>0.005</x-m>
            <y-m>-0.090</y-m>
            <z-m>0.094</z-m>
        </top-left>
    </panel>

</PropertyList>
